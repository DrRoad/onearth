<WMS_Tile_Service version="0.1.0">
  <Service>
    <Name>GIBS:WMS:Tile</Name>
    <Title>WMS Tile Service</Title>
    <Abstract>Tiled WMS service, tiled in a global grid</Abstract>
    <KeywordList>
      <Keyword>WMS</Keyword>
      <Keyword>Tile</Keyword>
      <Keyword>GIBS</Keyword>
      <Keyword>Global</Keyword>
      <Keyword>Earth</Keyword>
    </KeywordList>
    <OnlineResource xlink:href="http://localhost/twms-geo/" xlink:type="simple" xmlns:xlink="http://www.w3.orf/1999/xlink"/>
    <ContactInformation>
      <ContactPersonPrimary>
        <ContactPerson></ContactPerson>
        <ContactOrganization></ContactOrganization>
      </ContactPersonPrimary>
      <ContactElectronicMailAddress></ContactElectronicMailAddress>
    </ContactInformation>
    <Fees>none</Fees>
    <AccessConstraints>none</AccessConstraints>
  </Service>
  <TiledPatterns>
    <OnlineResource xlink:href="http://localhost/twms-geo/" xlink:type="simple" xmlns:xlink="http://www.w3.org/1999/xlink"/>
    <BoundingBox minx="-180" miny="-90" maxx="180" maxy="90" />
<TiledGroup>
    <Name>Blue Marble tileset</Name>
    <Title xml:lang="en">Blue Marble Demo Imagery</Title>
    <Abstract xml:lang="en">blue_marble abstract</Abstract>
    <Projection>GEOGCS["WGS 84",DATUM["WGS_1984",SPHEROID["WGS 84",6378137,298.257223563,AUTHORITY["EPSG","7030"]],AUTHORITY["EPSG","6326"]],PRIMEM["Greenwich",0,AUTHORITY["EPSG","8901"]],UNIT["degree",0.0174532925199433,AUTHORITY["EPSG","9122"]],AUTHORITY["EPSG","4326"]]</Projection>
    <Pad>0</Pad>
    <Bands>3</Bands>
    <BoundingBox minx="-180" miny="-90" maxx="180" maxy="90" />
    <Key>\${time}</Key>
 <TilePattern>
  <![CDATA[request=GetMap&layers=blue_marble&srs=EPSG:4326&format=image%2Fjpeg&styles=&time=${time}&width=512&height=512&bbox=-180,85.5,-175.5,90 request=GetMap&layers=blue_marble&srs=EPSG:4326&format=image%2Fjpeg&styles=&width=512&height=512&bbox=-180,85.5,-175.5,90]]>
</TilePattern>
