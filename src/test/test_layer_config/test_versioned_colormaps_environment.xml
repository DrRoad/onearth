    <ServiceURL service="twms">http://localhost/twms-geo/</ServiceURL>
    <LegendLocation>/etc/onearth/config/test/test_layer_config/legends</LegendLocation>
    <LegendURL>http://localhost/legends/</LegendURL>
    <ColorMapLocation version="1.0">/etc/onearth/config/test/test_layer_config/colormaps/1.0</ColorMapLocation>
    <ColorMapURL version="1.0">http://localhost/colormaps/1.0</ColorMapURL>
    <ColorMapLocation version="2.0" default="true">/etc/onearth/config/test/test_layer_config/colormaps/2.0</ColorMapLocation>
