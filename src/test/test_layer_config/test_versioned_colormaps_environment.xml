<?xml version="1.0" encoding="UTF-8"?>
<EnvironmentConfiguration>
    <GetCapabilitiesLocation service="wmts">/etc/onearth/config/test/test_layer_config/gc</GetCapabilitiesLocation>
    <GetCapabilitiesLocation service="twms">/etc/onearth/config/test/test_layer_config/gc/.lib/</GetCapabilitiesLocation>
    <GetTileServiceLocation>/etc/onearth/config/test/test_layer_config/gc/.lib/</GetTileServiceLocation>
    <CacheLocation service="wmts" basename="cache_all_wmts">/etc/onearth/config/test/test_layer_config/data</CacheLocation>
    <CacheLocation service="twms" basename="cache_all_twms">/etc/onearth/config/test/test_layer_config/data</CacheLocation>
